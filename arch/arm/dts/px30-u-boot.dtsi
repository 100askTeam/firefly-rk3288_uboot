/ {
	aliases {
		mmc0 = &emmc;
		mmc1 = &sdmmc;
	};
};

&uart2 {
	clock-frequency = <24000000>;
	u-boot,dm-pre-reloc;
	status = "okay";
};

&nandc0 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&sdmmc {
	u-boot,dm-pre-reloc;
};

&emmc {
	u-boot,dm-pre-reloc;
};

&pmugrf {
	u-boot,dm-pre-reloc;
};

&cru {
	u-boot,dm-pre-reloc;
};

&pmucru {
	u-boot,dm-pre-reloc;
};

&saradc {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&gpio0 {
	u-boot,dm-pre-reloc;
	status = "disabled";
};

&gpio1 {
	u-boot,dm-pre-reloc;
	status = "disabled";
};

&gpio2 {
	u-boot,dm-pre-reloc;
	status = "disabled";
};

&gpio3 {
	u-boot,dm-pre-reloc;
	status = "disabled";
};

&usb20_otg {
	u-boot,dm-pre-reloc;
};

&usb2phy_grf {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&u2phy {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&u2phy_otg {
	u-boot,dm-pre-reloc;
	status = "okay";
};
